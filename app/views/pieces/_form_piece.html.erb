<%= simple_form_for(piece) do |f|%>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.association :tags %>
  <%= f.input :photo, input_html: {accept: ('image') } %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, class: "btn btn-primary"%>
<% end %>
